<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="br.com.farofino.domain.categoria.TipoCaracteristica" table="TIPOCARACTERISTICA">
    <id column="ID" length="36" name="ID">
      <generator class="uuid2"/>
    </id>
    <property column="NOME" length="256" name="nome" not-null="true"/>
    <property column="IDENTIFICADOR" length="1" name="defining" not-null="true"/>
    <property column="TIPO" length="32" name="type" not-null="true"/>
  </class>
</hibernate-mapping>
