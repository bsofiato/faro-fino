<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="br.com.farofino.domain.produto.TipoCaracteristica" table="TIPOCARACTERISTICA">
    <id column="ID" length="36" name="ID">
      <generator class="uuid2"/>
    </id>
    <property name="nome" column="NOME" length="256" not-null="true"/>
    <property name="defining" column="IDENTIFICADOR" length="1" not-null="true"/>
  </class>
</hibernate-mapping>
